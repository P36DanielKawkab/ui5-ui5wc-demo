#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@ui5+webcomponents-tools@1.0.0-rc.15_chromedriver@94.0.0/node_modules/@ui5/webcomponents-tools/bin/init-ui5-package.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@ui5+webcomponents-tools@1.0.0-rc.15_chromedriver@94.0.0/node_modules/@ui5/webcomponents-tools/bin/init-ui5-package.js" "$@"
fi
