#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/npm@7.24.2/node_modules/npm/bin/npm-cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/npm@7.24.2/node_modules/npm/bin/npm-cli.js" "$@"
fi
