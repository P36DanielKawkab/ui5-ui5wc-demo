#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@ui5+webcomponents-tools@1.11.0_pe22slfhz44d3qjjag4fpkhtny/node_modules/@ui5/webcomponents-tools/bin/create-ui5-element.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@ui5+webcomponents-tools@1.11.0_pe22slfhz44d3qjjag4fpkhtny/node_modules/@ui5/webcomponents-tools/bin/create-ui5-element.js" "$@"
fi
