#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../lighthouse@8.6.0/node_modules/lighthouse/lighthouse-cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../../lighthouse@8.6.0/node_modules/lighthouse/lighthouse-cli/index.js" "$@"
fi
