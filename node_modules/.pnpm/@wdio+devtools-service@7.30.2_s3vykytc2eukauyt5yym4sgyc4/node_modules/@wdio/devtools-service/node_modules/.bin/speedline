#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../speedline@1.4.3/node_modules/speedline/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../speedline@1.4.3/node_modules/speedline/cli.js" "$@"
fi
