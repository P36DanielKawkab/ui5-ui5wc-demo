#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@wdio+cli@7.30.2_typescript@4.9.5/node_modules/@wdio/cli/bin/wdio.js" "$@"
else
  exec node  "$basedir/../../../../../../@wdio+cli@7.30.2_typescript@4.9.5/node_modules/@wdio/cli/bin/wdio.js" "$@"
fi
