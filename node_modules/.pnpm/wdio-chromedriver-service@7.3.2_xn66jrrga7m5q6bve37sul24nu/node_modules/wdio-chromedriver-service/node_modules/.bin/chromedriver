#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../chromedriver@94.0.0/node_modules/chromedriver/bin/chromedriver" "$@"
else
  exec node  "$basedir/../../../../../chromedriver@94.0.0/node_modules/chromedriver/bin/chromedriver" "$@"
fi
