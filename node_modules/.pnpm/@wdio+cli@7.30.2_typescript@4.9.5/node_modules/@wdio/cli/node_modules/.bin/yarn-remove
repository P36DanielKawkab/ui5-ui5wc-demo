#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../yarn-install@1.0.0/node_modules/yarn-install/bin/yarn-remove.js" "$@"
else
  exec node  "$basedir/../../../../../../yarn-install@1.0.0/node_modules/yarn-install/bin/yarn-remove.js" "$@"
fi
