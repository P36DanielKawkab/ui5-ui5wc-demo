#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../nps@5.10.0/node_modules/nps/dist/bin/nps.js" "$@"
else
  exec node  "$basedir/../../../../../../nps@5.10.0/node_modules/nps/dist/bin/nps.js" "$@"
fi
