#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../postcss-cli@9.1.0_postcss@8.4.21/node_modules/postcss-cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../../postcss-cli@9.1.0_postcss@8.4.21/node_modules/postcss-cli/index.js" "$@"
fi
