#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../concurrently@6.5.1/node_modules/concurrently/bin/concurrently.js" "$@"
else
  exec node  "$basedir/../../../../../../concurrently@6.5.1/node_modules/concurrently/bin/concurrently.js" "$@"
fi
