#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../mocha@10.2.0/node_modules/mocha/bin/_mocha" "$@"
else
  exec node  "$basedir/../../../../../../mocha@10.2.0/node_modules/mocha/bin/_mocha" "$@"
fi
